{
    "env": {
        "node": true
    },
    "globals": {
    },
    "rules": {
        "semi": [1, "always"],
        "quotes":[1, "single"],
        "no-unused-vars":1,
        "no-mixed-requires":0,
        "no-underscore-dangle":0,
        "no-extra-parens":1,
        "no-redeclare":2,
        "no-reserved-keys":1,
        "no-shadow":2,
        "valid-jsdoc":1,
        "default-case":1,
        "eqeqeq":2,
        "guard-for-in":2,
        "no-else-return":1,
        "no-warning-comments":1,
        "radix":2,
        "vars-on-top":0,
        "wrap-iife":2,
        "no-undefined":0,
        "handle-callback-err":2,
        "no-sync":1,
        "camelcase":0,
        "no-lonely-if":0,
        "space-infix-ops":0,
        "yoda":0,
        "no-void":2,
        "no-space-before-semi":0
    }
}
